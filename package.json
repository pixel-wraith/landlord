{
	"name": "landlord",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "eslint .",
		"db:start": "docker compose up",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:generate:tenant": "drizzle-kit generate --config=drizzle.tenant.config.ts",
		"db:migrate": "drizzle-kit migrate",
		"db:migrate:tenant": "tsx src/lib/server/db/scripts/apply-tenant-migrations.ts",

		"db:studio": "drizzle-kit studio",
		"db:psql": "psql -h localhost -p 5433 -U root -d local"
	},
	"devDependencies": {
		"@eslint/compat": "^1.2.5",
		"@eslint/js": "^9.18.0",
		"@sveltejs/adapter-node": "^5.2.12",
		"@sveltejs/kit": "^2.22.0",
		"@sveltejs/vite-plugin-svelte": "^6.0.0",
		"@types/node": "^22",
		"@types/pg": "^8.15.5",
		"drizzle-kit": "^0.30.6",
		"eslint": "^9.18.0",
		"eslint-plugin-svelte": "^3.0.0",
		"globals": "^16.0.0",
		"svelte": "^5.0.0",
		"svelte-check": "^4.0.0",
		"tsx": "^4.19.2",
		"typescript": "^5.0.0",
		"typescript-eslint": "^8.20.0",
		"vite": "^7.0.4"
	},
	"dependencies": {
		"dayjs": "^1.11.13",
		"dotenv": "^17.2.1",
		"dotenv-expand": "^12.0.2",
		"drizzle-orm": "^0.40.1",
		"drizzle-zod": "^0.8.3",
		"pg": "^8.16.3",
		"postgres": "^3.4.5",
		"zod": "^4.0.17"
	}
}
